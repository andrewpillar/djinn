{% collapsespace %}
{% func Render(a Alert, url string) %}
	{% if a.Message != "" %}
		<div class="alert alert-{%s a.Level.String() %}">
			<div class="alert-message">
				{% if a.HTML %}
					{%s= a.Message %}
				{% else %}
					{%s a.Message %}
				{% endif %}
			</div>
			{% if a.Close %}
				<a href="{%s url %}" class="alert-close">{% cat "../static/svg/close.svg" %}</a>
			{% endif %}
		</div>
	{% endif %}
{% endfunc %}
{% endcollapsespace %}
